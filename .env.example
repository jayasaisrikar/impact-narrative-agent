# ========================================
# Impact Narrative Agent - Environment Configuration
# Copy this to .env and fill in your actual values
# ========================================

# Gemini API Key - Get from https://ai.google.dev/
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Supabase Configuration - Get from your Supabase project settings
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Supabase Service Key (backend only, not exposed to browser)
SUPABASE_SERVICE_KEY=your_supabase_service_key

# Deno Agent Configuration (for local development)
VITE_CHAT_PROXY_URL=http://localhost:8787
DENO_AGENT_PORT=8787
